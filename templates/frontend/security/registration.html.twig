{% extends 'base.html.twig' %}

{% block title %}S'inscrire{% endblock %}

{% block body %}

<div class="login-form">

    <h1 class="h3 mb-3 font-weight-normal form">Inscrivez vous !</h1>

    {{ form_start(registration_form) }}
    
        {{ form_row(registration_form.email) }}

        {{ form_row(registration_form.password) }}

        <button type="submit" class="btn btn-lg btn-secondary">Inscription</button>

    {{ form_end(registration_form) }}
</div>

{% endblock %}